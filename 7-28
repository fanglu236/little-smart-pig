import java.util.Scanner;
 
public class Main{
	
	public static void main(String[] args){
		int[] nums = new int[10];
		Scanner cin = new Scanner(System.in);
		for(int i = 0; i < 10; ++i){
			nums[i] = cin.nextInt();
		}
		
		for(int i = 0; i < 10; ++i){
			for(int j = i+1; j < 10; ++j){
				System.out.print(nums[i] + "," + nums[j] + "  ");
			}
			System.out.println();
		}
	}
	
}
